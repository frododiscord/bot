{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": ["error", "tab"],
        "no-tabs": "off",
        "no-mixed-spaces-and-tabs": "off",
        "max-len": "off",
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "no-unused-vars": "warn",
		"no-invalid-this": "off"
    }
}
